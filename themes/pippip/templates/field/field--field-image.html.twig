{% set jpg= 'jpg' %}
{% set webp = 'webp' %}

{% for item in items %}

  {% set imageUrl = item.content['#item'].entity.uri.value %}
  {% set image = imageUrl|image_style('inline_image') %}

  <picture>
    {# <source data-srcset="{{ image|replace({ (jpg): webp }) }}" type="image/webp"> #}
    <source data-srcset="{{ image }}" type="image/jpeg">
    <img data-src="{{ image }}" alt="{{ item.content['#item'].alt }}" class="lazy" title="{{ item.content['#item'].title }}" />
  </picture>

{% endfor %}
